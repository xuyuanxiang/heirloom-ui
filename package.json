{
  "name": "heirloom-ui",
  "version": "1.0.0",
  "description": "heirloom ui write in react js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --hot --progress --colors --inline --content-base public/",
    "prestart:prod": "rm -rf public",
    "start:prod": "webpack --define process.env.NODE_ENV='\"production\"'",
    "prerelease": "rm -rf dist",
    "release": "NODE_ENV=release babel ./src --out-dir ./dist --ignore app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyuanxiang/heirloom-ui.git"
  },
  "keywords": [
    "React"
  ],
  "author": "xuyuanxiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xuyuanxiang/heirloom-ui/issues"
  },
  "homepage": "https://github.com/xuyuanxiang/heirloom-ui#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.24.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "inline-style-prefixer": "^2.0.1",
    "iscroll": "^5.2.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.15.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "peerDependencies": {
    "iscroll": "^5.2.0",
    "react": "^0.14.8 || ^15.0.0",
    "react-dom": "^0.14.8 || ^15.0.0"
  }
}
